{
  "name": "@sequencemedia/standard-server",
  "version": "1.0.7",
  "description": "Standard Server",
  "private": true,
  "main": "./app.js",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "http://sequencemedia.net"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production DEBUG=@sequencemedia* node app",
    "dev": "cross-env NODE_ENV=development concurrently -r \"npm run gulp\" \"webpack --config webpack.dev.js --watch\"",
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run gulp -- build\" \"webpack --env prod --config webpack.prod.js\"",
    "build:fonts": "source nvm.sh && npm run gulp -- build:fonts",
    "build:icons": "source nvm.sh && npm run gulp -- build:icons",
    "build:js": "source nvm.sh && webpack --config webpack.dev.js",
    "build:css": "source nvm.sh && npm run gulp -- build:css",
    "watch": "npm run gulp -- watch",
    "watch:fonts": "npm run gulp -- watch:fonts",
    "watch:icons": "npm run gulp -- watch:icons",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "npm run gulp -- watch:css",
    "transform": "npm run gulp -- transform",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint client/assets/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "test": "npm run test:jest && npm run test:mocha",
    "test:jest": "cross-env NODE_ENV=test jest --verbose --run-in-band",
    "test:mocha": "cross-env NODE_ENV=test mocha -r @babel/register test/server --recursive",
    "gulp": "source nvm.sh && gulp --color",
    "nvm": "bash nvm.sh",
    "pre-commit": "npm run lint && npm run test && npm run build && npm run transform && git add public server/views"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "@hapi/boom": "^9.1.4",
    "@hapi/hapi": "^20.2.1",
    "@hapi/inert": "^6.0.5",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^6.1.0",
    "@sequencemedia/react-router-redux-render": "^2.0.139",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "handlebars": "^4.7.7",
    "isomorphic-fetch": "^3.0.0",
    "moment": "^2.29.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-tab-set": "^1.1.137",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.0.0",
    "core-js": "^3.21.1",
    "cssnano": "^5.1.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.11.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "glob-all": "^3.3.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-develop-server": "^0.5.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "mocha": "^9.2.2",
    "module-alias": "^2.2.2",
    "postcss": "^8.4.12",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.3",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "stylelint": "^14.6.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-scss": "^4.2.0",
    "terser-webpack-plugin": "^5.3.1",
    "vinyl-paths": "3.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "_moduleAliases": {
    "~": ".",
    "@sequencemedia/api": "./client/assets/js/api",
    "@sequencemedia/app": "./client/assets/js/app"
  }
}
