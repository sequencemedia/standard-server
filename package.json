{
  "name": "@sequencemedia/standard-server",
  "version": "1.0.3",
  "description": "Standard Server",
  "private": true,
  "main": "./app.js",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "http://sequencemedia.net"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=12.19.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production DEBUG=@sequencemedia* node app",
    "dev": "cross-env NODE_ENV=development concurrently -r \"npm run gulp\" \"webpack --config webpack.dev.js --watch\"",
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run gulp -- build\" \"webpack --env prod --config webpack.prod.js\"",
    "build:fonts": "npm run gulp -- build:fonts",
    "build:icons": "npm run gulp -- build:icons",
    "build:js": "webpack --config webpack.dev.js",
    "build:css": "npm run gulp -- build:css",
    "watch": "npm run gulp -- watch",
    "watch:fonts": "npm run gulp -- watch:fonts",
    "watch:icons": "npm run gulp -- watch:icons",
    "watch:js": "npm run build:js -- --watch",
    "watch:css": "npm run gulp -- watch:css",
    "transform": "npm run gulp -- transform",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint client/assets/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "test": "npm run test:jest && npm run test:mocha",
    "test:jest": "cross-env NODE_ENV=test jest --verbose --run-in-band",
    "test:mocha": "cross-env NODE_ENV=test mocha -r @babel/register test/server --recursive",
    "gulp": "gulp --color",
    "pre-commit": "npm run lint && npm run test && npm run build && npm run transform && git add public server/views"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^20.0.3",
    "@hapi/inert": "^6.0.3",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^6.0.1",
    "@sequencemedia/react-router-redux-render": "^2.0.0",
    "cross-env": "^7.0.2",
    "debug": "^4.3.1",
    "handlebars": "^4.7.6",
    "isomorphic-fetch": "^3.0.0",
    "moment": "^2.29.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-tab-set": "^1.1.5",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.7.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^5.0.0",
    "glob-all": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-develop-server": "^0.5.2",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "module-alias": "^2.2.2",
    "postcss": "^8.1.10",
    "postcss-normalize": "^9.0.0",
    "postcss-scss": "^3.0.4",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^5.0.3",
    "vinyl-paths": "^3.0.1",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.4.0"
  },
  "_moduleAliases": {
    "~": ".",
    "@sequencemedia/api": "./client/assets/js/api",
    "@sequencemedia/app": "./client/assets/js/app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
